<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="false"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalendarView"
        tools:targetApi="31">
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CalendarView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.ui.HorizontalCalendarActivity"
            android:exported="false" />
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.ui.VerticalCalendarActivity"
            android:exported="false" />
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.ui.SingleCalendarActivity"
            android:exported="false" />
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.ui.MultipleCalendarActivity"
            android:exported="false" />
        <activity
            android:name="com.wyyzlxg.calendarviewdemo.ui.RangeCalendarActivity"
            android:exported="false" />
    </application>

</manifest>